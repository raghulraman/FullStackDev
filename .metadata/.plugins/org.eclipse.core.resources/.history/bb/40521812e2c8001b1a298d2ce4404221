package rough.work;

public class Rough {
	
	//static int[] arr= {5,2,9,4,1,8,7,3,6};
	
	void swap(int x, int y){
		int temp=x;
		x=y;
		y=temp;
	}
	
	void quickSorting(int[] array, int left, int right){
		if(right-left<=0) return;
		int pivot = array[(left+right)/2]; int i=left; int j=right;
		while(i<=j){
			while(array[i]<pivot) i++;
			while(array[j]>pivot) j--;
			if(i<=j) {
				swap(array[i],array[j]);
				i++; j--;
			}
		}
		quickSorting(array,left,i-1);
		quickSorting(array,i,right);
	}

	public static void main(String[] args) {
		Rough r1 = new Rough();
		int[] arr= {5,2,9,4,1,8,7,3,6};
		r1.quickSorting(arr, 0, 9);
		for(int k:arr) {
			System.out.println(k);
		}
	}

}
