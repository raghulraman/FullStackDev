package java.training;

public class Calculator {
	
	static int a,b,k;

	public static void main(String[] args) {
		a=Integer.parseInt(args[1]);
		b=Integer.parseInt(args[2]);
		
	}
	
	int add() {
		k=a+b;
		return k;
	}
	int sub() {
		k=a-b;
		return k;
	}
	int mul() {
		k=a*b;
		return k;
	}
	int div() {
		k=a/b;
		return k;
	}

}
